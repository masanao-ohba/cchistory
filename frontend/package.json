{
  "name": "claude-conversations-viewer-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Frontend for Claude Conversations History Viewer",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:debug": "VITE_DEBUG=true vite --host 0.0.0.0",
    "dev:prod": "VITE_DEBUG=false vite --host 0.0.0.0",
    "build": "vite build",
    "build:debug": "VITE_DEBUG=true vite build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.0",
    "markdown-it": "^14.1.0",
    "pinia": "^2.1.0",
    "vue": "^3.3.0",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
